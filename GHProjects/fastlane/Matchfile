git_url(ENV['FASTLANE_MATCH_GIT_REPO'])

storage_mode("git")

# The default type, can be: appstore, adhoc, enterprise or development
type("development")

# The bundle identifier of your app
app_identifier(ENV['FASTLANE_APP_IDENTIFIER'])

 # Your Apple Developer Portal username
username(ENV['FASTLANE_APPLE_ID'])

# For all available options run `fastlane match --help`
# Remove the # in the beginning of the line to enable the other options

# The docs are available on https://docs.fastlane.tools/actions/match